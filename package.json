{
  "name": "@hyrious/import-cost",
  "version": "0.1.2",
  "description": "Get package size of import statements, powered by esbuild",
  "author": "hyrious <hyrious@outlook.com>",
  "license": "MIT",
  "keywords": [
    "import",
    "gzip",
    "size",
    "bundle",
    "esbuild"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "bin": "cli.mjs",
  "files": [
    "src",
    "dist",
    "cli.mjs"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean --treeshake --target esnext --dts"
  },
  "peerDependencies": {
    "esbuild": "*"
  },
  "dependencies": {
    "@babel/parser": "^7.21.4",
    "@babel/traverse": "^7.21.4",
    "@babel/types": "^7.21.4",
    "builtin-modules": "^3.3.0",
    "escalade": "^3.1.1",
    "gzip-size": "^7.0.0",
    "pretty-bytes": "^6.1.0",
    "sade": "^1.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@types/node": "^18.15.11",
    "esbuild": "^0.17.17",
    "eslint": "^8.38.0",
    "lodash-es": "^4.17.21",
    "tsup": "^6.7.0",
    "types-babel-traverse": "npm:@types/babel__traverse@7.18.3",
    "typescript": "^5.0.4"
  }
}
